<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - DailyMart</title>
    @* @Styles.Render("~/Content/css")*@
    @Styles.Render("~/Content/stylesheets/css")
    @Scripts.Render("~/bundles/modernizr")
    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
</head>
<body class="header_sticky header-style-2 has-menu-extra">
    <!-- Preloader -->
    <div id="loading-overlay">
        <div class="loader"></div>
    </div>
    <!-- Boxed -->
    <div class="boxed">
        <div id="site-header-wrap">
            <!-- Header -->
            <header id="header" class="header clearfix">
                <div class="container-fluid clearfix container-width-93" id="site-header-inner">

                    <div class="mobile-button"><span></span></div>
                    <div id="logo" class="logo float-left">
                        <a href="@Url.Action("Index", "Home")" title="logo">
                            <img src="~/Content/Products/images/system/logo1x.png" alt="image" width="107" height="24" data-retina="Content/Products/images/system/logo2x.png" data-width="107" data-height="24">
                        </a>
                    </div><!-- /.logo -->
                    <ul class="menu-extra">

                        @Html.Partial("_LoginPartial")

                        <li class="box-cart nav-top-cart-wrapper">
                            @{ var count = 0;
                                if (Session["cart"] != null)
                                {
                                    // List<Item> cart = (List<Item>)Session["cart"];
                                    // count = cart.Count();
                                    count = 0;

                                }
                            }
                            <a class="icon_cart nav-cart-trigger active" href="@Url.Action("Cart", "Home")"><span id="cartProductsCount">@count</span></a>
                        </li>
                    </ul>
                    <!-- /.menu-extra -->
                    <div class="nav-wrap">
                        <nav id="mainnav" class="mainnav">
                            <ul class="menu">
                                <li><a href="@Url.Action("Index", "Home")">Home</a></li>
                                @if (Request.IsAuthenticated)
                                {
                                    <li><a href="@Url.Action("Index","Home"@*,new { userId=User.Identity.GetUserId()}*@)">Orders</a></li>
                                }
                                <li><a href="@Url.Action("Contact", "Home")">Contact</a></li>
                                <li><a href="@Url.Action("About", "Home")">About</a></li>
                            </ul>
                        </nav><!-- /.mainnav -->
                    </div><!-- /.nav-wrap -->
                </div><!-- /.container-fluid -->
            </header><!-- /header -->
        </div><!-- /.site-header-wrap -->


        @RenderBody()

        <section class="flat-row mail-chimp">
            <div class="container">
                <div class="row">
                    <div class="col-md-4">
                        <div class="text">
                            <h3>Sign up for Send Newsletter</h3>
                        </div>
                    </div>
                    <div class="col-md-8">
                        <div class="subscribe clearfix">
                            <form action="#" method="post" accept-charset="utf-8" id="subscribe-form">
                                <div class="subscribe-content">
                                    <div class="input">
                                        <input type="email" name="subscribe-email" placeholder="Your Email">
                                    </div>
                                    <div class="button">
                                        <button type="button">SUBCRIBE</button>
                                    </div>
                                </div>
                            </form>
                            <ul class="flat-social">
                                <li><a href="@Url.Action("Error404", "Home")"><i class="fa fa-facebook"></i></a></li>
                                <li><a href="@Url.Action("Error404", "Home")"><i class="fa fa-twitter"></i></a></li>
                                <li><a href="@Url.Action("Error404", "Home")"><i class="fa fa-google"></i></a></li>
                                <li><a href="@Url.Action("Error404", "Home")"><i class="fa fa-behance"></i></a></li>
                                <li><a href="@Url.Action("Error404", "Home")"><i class="fa fa-linkedin"></i></a></li>
                            </ul><!-- /.flat-social -->
                        </div><!-- /.subscribe -->
                    </div>
                </div>
            </div>
        </section><!-- /.mail-chimp -->
        <!-- Footer -->
        <footer class="footer">
            <div class="container">
                <div class="row">
                    <div class="col-md-3">
                        <div class="widget widget-link">
                            <ul>
                                <li><a href="@Url.Action("About", "Home")">About Us</a></li>
                                <li><a href="@Url.Action("Error404", "Home")">Online Store</a></li>
                                <li><a href="@Url.Action("Error404", "Home")">Blog</a></li>
                                <li><a href="@Url.Action("Contact", "Home")">Contact Us</a></li>
                            </ul>
                        </div><!-- /.widget -->
                    </div><!-- /.col-md-3 -->
                    <div class="col-md-3">
                        <div class="widget widget-link link-login">
                            <ul>
                                <li><a href="@Url.Action("Login", "Account")">Login/ Register</a></li>
                                    @*<li><a href="@Url.Action("Cart", "Home")">Your Cart</a></li>
                                    <li><a href="@Url.Action("MyOrders", "Home")">Your checkout</a></li>
    *@
                            </ul>
                        </div><!-- /.widget -->
                    </div><!-- /.col-md-3 -->
                    <div class="col-md-3">
                        <div class="widget widget-link link-faq">
                            <ul>
                                <li><a href="@Url.Action("Error404", "Home")">FAQs</a></li>
                                <li><a href="@Url.Action("Error404", "Home")">Term of service</a></li>
                                <li><a href="@Url.Action("Error404", "Home")">Privacy Policy</a></li>
                                <li><a href="@Url.Action("Error404", "Home")">Returns</a></li>
                            </ul>
                        </div><!-- /.widget -->
                    </div><!-- /.col-md-3 -->
                    <div class="col-md-3">
                        <div class="widget widget-brand">
                            <div class="logo logo-footer">
                                <a href="@Url.Action("Index", "Home")"><img src="~/Content/Products/images/system/logo2x.png" alt="image" width="107" height="24"></a>
                            </div>
                            <ul class="flat-contact">
                                <li class="address">304 ,Aster Blossam Aura College Road ,Nadiad</li>
                                <li class="phone">+91 9727254298</li>
                                <li class="email">parthpatel@gmail.com</li>
                            </ul><!-- /.flat-contact -->
                        </div><!-- /.widget -->
                    </div><!-- /.col-md-3 -->
                </div><!-- /.row -->
            </div><!-- /.container -->
        </footer><!-- /.footer -->

        <div class="footer-bottom">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <p class="copyright text-center"> <a href="/">Modaz</a></p>
                    </div>
                </div>
            </div>
        </div>

        <!-- Go Top -->
        <a class="go-top">
            <i class="fa fa-chevron-up"></i>
        </a>

    </div>

    @Scripts.Render("~/bundles/javascript/js")
    @Scripts.Render("~/bundles/rev-slider/js")
    @RenderSection("scripts", required: false)
</body>

</html>
